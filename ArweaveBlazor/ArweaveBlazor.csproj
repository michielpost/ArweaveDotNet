<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
	  <TargetFramework>net9.0</TargetFramework>
	  <Nullable>enable</Nullable>
	  <WarningsAsErrors>nullable</WarningsAsErrors>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <IncludeSymbols>True</IncludeSymbols>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <Authors>Michiel Post</Authors>
	  <Version>0.1.1</Version>
	  <Product>ArweaveBlazor</Product>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageProjectUrl>https://github.com/michielpost/ArweaveDotNet</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/michielpost/ArweaveDotNet</RepositoryUrl>
	  <RepositoryType>github</RepositoryType>
	  <PackageTags>arweave blockchain web3 blazor</PackageTags>
	  <Description>Arweave Blazor Extension</Description>
  </PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="9.0.2" />
  </ItemGroup>

</Project>
